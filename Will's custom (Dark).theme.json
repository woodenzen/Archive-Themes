            "color": "#f2ae33",
            "syntax": {
                "color": "#f2ae33"
            }
        },
        "h2": {
            "font": {
                "name": "SFMono-Regular",
                "size": 22
            },
            "color": "#f2ae33",
            "syntax": {
                "color": "#f2ae33"
            }
        },
        "h3": {
            "font": {
                "name": "SFMono-Regular",
                "size": 20
            },
            "color": "#f2ae33",
            "syntax": {
                "color": "#f2ae33"
            }
        },
        "h4": {
            "font": {
                "style": "bold",
                "name": "SFMono-Regular"
