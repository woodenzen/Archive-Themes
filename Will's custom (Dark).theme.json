{
    "name": "Custom Theme - Will",
    "style": "Dark",
    "author": [
        {
            "name": "Will Simpson",
            "url": "http://kestrelcreek.com.com/",
            "email": "linux.photo.geek@gmail.com",
            "comment": "For best results, download Apple's San Francisco font (https://developer.apple.com/fonts/) and set The Archive's font to 'SFMono-Regular'. Last updated: April 11, 2023 . Adopted for the visually impaired, mimicking the 'Panic Mood' theme available in the Bear app."
        }
    ],
    "version": "1.0",
    "editor": {
        "backgroundColor": "#131a25",
        "caretColor": "#4AA8FB",
        "selection": {
            "color": "#DDDDDD",
            "backgroundColor": "#3c95fa",
            "unfocusedBackgroundColor": "#3c95fa"
        },
        "highlight": {
            "color": "#2C2D2F",
            "backgroundColor": "#587083",
            "unfocusedBackgroundColor": "#4292D2"
        }
    },
    "savedSearchesSidebar": {
        "color": "#A5A6A6",
        "backgroundColor": "#131a25",
        "highlightedBackgroundColor": "#4292D2",
        "border": true,
        "iconColors": {
            "default": "#A5A6A6"
        }
    },
    "resultsList": {
